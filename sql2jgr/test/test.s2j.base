#define COUNT_CURVE(patt, patt2)                       \
 curve pattern patt {				       \
   sql = SELECT version_name, release_date, sum(data) as data       \
       	 	FROM "Count evol"                      \
       	 	WHERE standardized_name = patt2        \
		AND release_date >= '2003-12-18'       \
		GROUP BY version_name, release_date    \
		ORDER BY release_date;                 \
 }

