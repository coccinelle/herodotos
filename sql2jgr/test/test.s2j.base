/* **********  DIRECTORIES  ******************* */

pattern Linux {
 marktype = none
 color = 0 0 0
}

pattern Drivers {
 marktype = "circle fill 1"
 color = 1 0 0
}

pattern Staging {
 marktype = box
 color = 0 0 0
}

pattern Sound   {
 marktype = diamond
 color = 0 0 1
}

pattern Arch    {
 marktype = triangle
 color = 0 1 1
}

pattern FS      {
 marktype = x
 color = 0 1 0
}

pattern Net     {
 marktype = cross
 color = 1 0.69 0
}

pattern	Other   {
 color = 1 0 1
 marktype = "triangle fill 1"
}

pattern	Average   {
 color = 0 0 0
 marktype = none
 linestyle = dotted
}

/* ***********  CHECKERS  ********************* */

pattern Block {
 color = 0 1 0
 marktype = circle
}
pattern Null {
 color = 1 0 1
 marktype = cross
}
pattern Var {
 color = 0 1 .5
 marktype = box
}
pattern IsNull {
 color = 1 0.69 0
 marktype = diamond
}
pattern NullRef {
 color = 1 0 0
 marktype = diamond
}
pattern Range {
 color = 0 0 0
 marktype = triangle
}
pattern Lock {
 color = .5 0 1
 marktype = "circle fill 1"
}
pattern Intr {
 color = 0 .5 1
 marktype = cross
}
pattern LockIntr {
 color = .5 .5 1
 marktype = x
}
pattern Free {
 color = .5 0 0
 marktype = "box fill 1"	
}
pattern Float {
 color =  0 .5 0
 marktype = diamond
}
pattern Real {
 color = 0 0 .5
 marktype = none
}
pattern Param {
 color = 0 0 1
 marktype = none
}
pattern Size {
 color = 0 1 1
 marktype = triangle
}

/* ******************************************** */

pattern Birth {
 color = 1 0 0
 marktype = x
}

pattern Death {
 color = 0 1 0
 marktype = circle
}

