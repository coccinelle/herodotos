   sql = SELECT version_name, release_date, number_of_bugs_already_present       \
       	 	FROM "Origin of bugs"                  \
       	 	WHERE standardized_name = patt2        \
		AND for_version_name='linux-2.6.33'    \
		ORDER BY release_date;                 \
 }

