#define ACROSS_CURVE(patt, vers)          \
 curve pattern patt {                     \
   sql = SELECT "version_name",           \
          "release_date",		  \
          sum("number_of_bugs")		  \
   FROM faults_across_releases            \
   WHERE "for_version_name" = vers        \
   AND release_date >= '2003-12-18'	  \
   AND standardized_name != 'Null'	  \
   GROUP BY "version_name","release_date" \
   ORDER BY release_date;                 \
 }

graph gr/lifetime_across.jgr {
 xaxis = date
 yaxis = user
 project = Linux
 size = 3 3.5
 legend = "defaults fontsize 6"
 ylegend = "Number of faults"

 curve pattern Linux {
  legend = "Total"
  color = 0 0 0
   sql = SELECT r.version_name, r.release_date,
       count(report_id)
FROM "Reports info" r, versions
WHERE versions.version_name = r.version_name
AND versions.main = 2
AND versions.major = 6
AND r.standardized_name != 'Null'
AND r.status = 'BUG'
GROUP BY r.version_name , r.release_date
ORDER BY r.release_date;
 }

